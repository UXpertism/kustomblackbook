<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <title>Kustom Black Book Vehicle Request</title>
  <!-- Bootstrap CSS -->
  <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha1/dist/css/bootstrap.min.css" rel="stylesheet">
</head>
<body>
  <div class="container mt-5">
    <h1 class="text-center">Custom Vehicle Request</h1>
    <form action="/results" method="POST">
      
      <!-- Basic Information -->
      <div class="mb-3">
        <label for="name" class="form-label">Name:</label>
        <input type="text" id="name" name="name" class="form-control" required>
      </div>

      <div class="mb-3">
        <label for="email" class="form-label">Email:</label>
        <input type="email" id="email" name="email" class="form-control" required>
      </div>

      <div class="mb-3">
        <label for="phone" class="form-label">Phone:</label>
        <input type="tel" id="phone" name="phone" class="form-control" required>
      </div>

      <!-- Audio System -->
      <h3 class="mt-4">Audio System</h3>

      <div class="mb-3">
        <label for="amp" class="form-label">Amplifier:</label>
        <input type="text" id="amp" name="amp" class="form-control">
      </div>

      <div class="mb-3">
        <label for="amp_size" class="form-label">Amplifier Size/Placement:</label>
        <input type="text" id="amp_size" name="amp_size" class="form-control">
      </div>

      <!-- Body Information -->
      <h3 class="mt-4">Body Information</h3>
      <div class="mb-3">
        <label for="bumpers" class="form-label">Bumpers:</label>
        <input type="text" id="bumpers" name="bumpers" class="form-control">
      </div>

      <div class="mb-3">
        <label for="doors" class="form-label">Doors:</label>
        <input type="text" id="doors" name="doors" class="form-control">
      </div>

      <div class="mb-3">
        <label for="fenders" class="form-label">Fenders:</label>
        <input type="text" id="fenders" name="fenders" class="form-control">
      </div>

      <div class="mb-3">
        <label for="quarters" class="form-label">Quarters:</label>
        <input type="text" id="quarters" name="quarters" class="form-control">
      </div>

      <!-- Chassis Information -->
      <h3 class="mt-4">Chassis Information</h3>

      <div class="mb-3">
        <label for="brakes" class="form-label">Brakes:</label>
        <input type="text" id="brakes" name="brakes" class="form-control">
      </div>

      <div class="mb-3">
        <label for="brake_system" class="form-label">Brake System:</label>
        <input type="text" id="brake_system" name="brake_system" class="form-control">
      </div>

      <div class="mb-3">
        <label for="chassis" class="form-label">Chassis:</label>
        <input type="text" id="chassis" name="chassis" class="form-control">
      </div>

      <div class="mb-3">
        <label for="lower_control_arm" class="form-label">Lower Control Arm:</label>
        <input type="text" id="lower_control_arm" name="lower_control_arm" class="form-control">
      </div>

      <div class="mb-3">
        <label for="steering" class="form-label">Steering:</label>
        <input type="text" id="steering" name="steering" class="form-control">
      </div>

      <div class="mb-3">
        <label for="suspension" class="form-label">Suspension:</label>
        <input type="text" id="suspension" name="suspension" class="form-control">
      </div>

      <div class="mb-3">
        <label for="upper_control_arm" class="form-label">Upper Control Arm:</label>
        <input type="text" id="upper_control_arm" name="upper_control_arm" class="form-control">
      </div>

      <!-- Exterior Information -->
      <h3 class="mt-4">Exterior Information</h3>

      <div class="mb-3">
        <label for="engine_bay" class="form-label">Engine Bay:</label>
        <input type="text" id="engine_bay" name="engine_bay" class="form-control">
      </div>

      <div class="mb-3">
        <label for="hood" class="form-label">Hood:</label>
        <input type="text" id="hood" name="hood" class="form-control">
      </div>

      <div class="mb-3">
        <label for="top" class="form-label">Top:</label>
        <input type="text" id="top" name="top" class="form-control">
      </div>

      <div class="mb-3">
        <label for="trunk_lid" class="form-label">Trunk Lid:</label>
        <input type="text" id="trunk_lid" name="trunk_lid" class="form-control">
      </div>

      <div class="mb-3">
        <label for="trunk_pan" class="form-label">Trunk Pan:</label>
        <input type="text" id="trunk_pan" name="trunk_pan" class="form-control">
      </div>

      <div class="mb-3">
        <label for="tub" class="form-label">Tub:</label>
        <input type="text" id="tub" name="tub" class="form-control">
      </div>

      <!-- Glass Information -->
      <h3 class="mt-4">Glass Information</h3>

      <div class="mb-3">
        <label for="rear_glass" class="form-label">Rear Glass:</label>
        <input type="text" id="rear_glass" name="rear_glass" class="form-control">
      </div>

      <div class="mb-3">
        <label for="windows" class="form-label">Windows:</label>
        <input type="text" id="windows" name="windows" class="form-control">
      </div>

      <div class="mb-3">
        <label for="windshield" class="form-label">Windshield:</label>
        <input type="text" id="windshield" name="windshield" class="form-control">
      </div>

      <!-- Interior Information -->
      <h3 class="mt-4">Interior Information</h3>

      <div class="mb-3">
        <label for="air_heat" class="form-label">Air/Heat:</label>
        <input type="text" id="air_heat" name="air_heat" class="form-control">
      </div>

      <div class="mb-3">
        <label for="carpet" class="form-label">Carpet:</label>
        <input type="text" id="carpet" name="carpet" class="form-control">
      </div>

      <div class="mb-3">
        <label for="console" class="form-label">Console:</label>
        <input type="text" id="console" name="console" class="form-control">
      </div>

      <div class="mb-3">
        <label for="dash_tacs" class="form-label">Dash/Tacs/Meters:</label>
        <input type="text" id="dash_tacs" name="dash_tacs" class="form-control">
      </div>

      <div class="mb-3">
        <label for="door_panels" class="form-label">Door Panels:</label>
        <input type="text" id="door_panels" name="door_panels" class="form-control">
      </div>

      <div class="mb-3">
        <label for="glove_box" class="form-label">Glove Box:</label>
        <input type="text" id="glove_box" name="glove_box" class="form-control">
      </div>

      <div class="mb-3">
        <label for="interior" class="form-label">Interior:</label>
        <input type="text" id="interior" name="interior" class="form-control">
      </div>

      <div class="mb-3">
        <label for="power_locks" class="form-label">Power Locks:</label>
        <input type="text" id="power_locks" name="power_locks" class="form-control">
      </div>

      <div class="mb-3">
        <label for="power_window" class="form-label">Power Window:</label>
        <input type="text" id="power_window" name="power_window" class="form-control">
      </div>

      <div class="mb-3">
        <label for="roof" class="form-label">Roof:</label>
        <input type="text" id="roof" name="roof" class="form-control">
      </div>

      <div class="mb-3">
        <label for="seats" class="form-label">Seats:</label>
        <input type="text" id="seats" name="seats" class="form-control">
      </div>

      <div class="mb-3">
        <label for="steering_wheel" class="form-label">Steering Wheel:</label>
        <input type="text" id="steering_wheel" name="steering_wheel" class="form-control">
      </div>

      <!-- Paint Information -->
      <h3 class="mt-4">Paint Information</h3>

      <div class="mb-3">
        <label for="paint_color" class="form-label">Paint Color:</label>
        <input type="text" id="paint_color" name="paint_color" class="form-control">
      </div>

      <div class="mb-3">
        <label for="paint_finish" class="form-label">Paint Finish:</label>
        <input type="text" id="paint_finish" name="paint_finish" class="form-control">
      </div>

      <div class="mb-3">
        <label for="paint_type" class="form-label">Paint Type:</label>
        <input type="text" id="paint_type" name="paint_type" class="form-control">
      </div>

      <!-- Transmission Information -->
      <h3 class="mt-4">Transmission Information</h3>

      <div class="mb-3">
        <label for="transmission" class="form-label">Transmission:</label>
        <input type="text" id="transmission" name="transmission" class="form-control">
      </div>

      <div class="mb-3">
        <label for="transmission_type" class="form-label">Manual/Automatic:</label>
        <input type="text" id="transmission_type" name="transmission_type" class="form-control">
      </div>

      <!-- Vehicle Information -->
      <h3 class="mt-4">Vehicle Information</h3>

      <div class="mb-3">
        <label for="boost" class="form-label">Boost:</label>
        <input type="text" id="boost" name="boost" class="form-control">
      </div>

      <div class="mb-3">
        <label for="engine" class="form-label">Engine:</label>
        <input type="text" id="engine" name="engine" class="form-control">
      </div>

      <div class="mb-3">
        <label for="make" class="form-label">Make:</label>
        <input type="text" id="make" name="make" class="form-control">
      </div>

      <div class="mb-3">
        <label for="model" class="form-label">Model:</label>
        <input type="text" id="model" name="model" class="form-control">
      </div>

      <div class="mb-3">
        <label for="posi" class="form-label">Posi:</label>
        <input type="text" id="posi" name="posi" class="form-control">
      </div>

      <div class="mb-3">
        <label for="rear_diff" class="form-label">Rear Differential:</label>
        <input type="text" id="rear_diff" name="rear_diff" class="form-control">
      </div>

      <div class="mb-3">
        <label for="awd_4x4" class="form-label">AWD/4x4:</label>
        <input type="text" id="awd_4x4" name="awd_4x4" class="form-control">
      </div>

      <div class="mb-3">
        <label for="start_option" class="form-label">Key/Push Button/Start Option:</label>
        <input type="text" id="start_option" name="start_option" class="form-control">
      </div>

      <div class="mb-3">
        <label for="type_badge" class="form-label">Specific/Type/Badge:</label>
        <input type="text" id="type_badge" name="type_badge" class="form-control">
      </div>

      <div class="mb-3">
        <label for="year" class="form-label">Year:</label>
        <input type="text" id="year" name="year" class="form-control">
      </div>

      <!-- Wheels and Tires -->
      <h3 class="mt-4">Wheels and Tires</h3>

      <div class="mb-3">
        <label for="tire_size" class="form-label">Tire Size:</label>
        <input type="text" id="tire_size" name="tire_size" class="form-control">
      </div>

      <div class="mb-3">
        <label for="wheel_brand" class="form-label">Wheel Brand:</label>
        <input type="text" id="wheel_brand" name="wheel_brand" class="form-control">
      </div>

      <div class="mb-3">
        <label for="wheel_offset" class="form-label">Wheel Offset:</label>
        <input type="text" id="wheel_offset" name="wheel_offset" class="form-control">
      </div>

      <div class="mb-3">
        <label for="wheel_stagger" class="form-label">Wheel Stagger:</label>
        <input type="text" id="wheel_stagger" name="wheel_stagger" class="form-control">
      </div>

      <div class="mb-3">
        <label for="wheel_type" class="form-label">Wheel Type:</label>
        <input type="text" id="wheel_type" name="wheel_type" class="form-control">
      </div>

      <div class="mb-3">
        <label for="wheels_size" class="form-label">Wheel Size:</label>
        <input type="text" id="wheels_size" name="wheels_size" class="form-control">
      </div>

      <button type="submit" class="btn btn-primary">Submit Request</button>
    </form>
  </div>

  <!-- Bootstrap JS (Optional for some components) -->
  <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha1/dist/js/bootstrap.bundle.min.js"></script>
</body>
</html>
